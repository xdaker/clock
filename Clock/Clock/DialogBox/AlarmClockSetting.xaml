<UserControl x:Class="Clock.DialogBox.AlarmClockSetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Clock.DialogBox"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="ViewGrid">
        <Image x:Name="ImageOk" HorizontalAlignment="Left" Height="45" Margin="30,0,0,0" VerticalAlignment="Bottom" Width="45" Source="/Clock;component/Imge/ok_normal.png"/>
        <Image x:Name="ImageCancel" HorizontalAlignment="Right" Height="45" Margin="0,0,30,0" VerticalAlignment="Bottom" Width="45" Source="/Clock;component/Imge/cancel_normal.png"/>

    </Grid>
</UserControl>
